<?xml version="1.0" encoding="utf-8"?>
<!-- edited with XMLSpy v2022 rel. 2 (x64) (http://www.altova.com) by  (FGBU «FKP Rosreestra») -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:iBND2="urn://x-artefacts-rosreestr-ru/interact/interact-entry-boundaries/2.0.1"
           xmlns:SimAll1="urn://x-artefacts-rosreestr-ru/commons/simple-types-all/1.0"
           xmlns:Are1="urn://x-artefacts-rosreestr-ru/commons/complex-types/area/1.0.1"
           xmlns:EnSpa2="urn://x-artefacts-rosreestr-ru/commons/complex-types/entity-spatial/2.0.1"
           xmlns:RLaw1="urn://x-artefacts-rosreestr-ru/commons/complex-types/resolution-requisites-law/1.0.1"
           xmlns:AdrCi2="urn://x-artefacts-rosreestr-ru/commons/complex-types/address-city/2.0.1"
           xmlns:dBnd3="urn://x-artefacts-rosreestr-ru/commons/directories/boundary-type/3.0.1"
           xmlns:dWtr1="urn://x-artefacts-rosreestr-ru/commons/directories/water-object-type/1.0.1"
           xmlns:dAU="urn://x-artefacts-rosreestr-ru/commons/directories/allowed-use/4.0.1"
           xmlns:dParc2="urn://x-artefacts-rosreestr-ru/commons/directories/parcels/2.0.1"
           xmlns:dZo4="urn://x-artefacts-rosreestr-ru/commons/directories/zone/4.0.1"
           xmlns:dMeth2="urn://x-artefacts-rosreestr-ru/commons/directories/method/2.0.1"
           xmlns:con1="http://rosreestr.ru/services/egrn/1.0.1/extract/common/contacts"
           xmlns:h2="urn://x-artefacts-rosreestr-ru/commons/complex-types/holder/2.0.1"
           xmlns:dPubEas="urn://x-artefacts-rosreestr-ru/commons/directories/purpose-public-easement/1.0.1"
           xmlns:dCateg1="urn://x-artefacts-rosreestr-ru/commons/directories/categories/1.0.1"
           targetNamespace="urn://x-artefacts-rosreestr-ru/interact/interact-entry-boundaries/2.0.1"
           elementFormDefault="qualified">
    <!--ПРОСТЫЕ ТИПЫ ДАННЫХ:-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/simple-types-all/1.0"
               schemaLocation="P_CommonSimpleTypeAll_v01.xsd"/>
    <!--СОСТАВНЫЕ ТИПЫ ДАННЫХ:-->
    <!--Площадь-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/complex-types/area/1.0.1"
               schemaLocation="tArea_v01.xsd"/>
    <!--Описание местоположения границ-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/complex-types/entity-spatial/2.0.1"
               schemaLocation="tEntitySpatial_v02.xsd"/>
    <!--Реквизиты документа (федерального закона)-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/complex-types/resolution-requisites-law/1.0.1"
               schemaLocation="tResolutionRequisitesLaw_v01.xsd"/>
    <!--Структурированный адрес (описание местоположения) до уровня населённого пункта-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/complex-types/address-city/2.0.1"
               schemaLocation="tAddressCity_v02.xsd"/>
    <!--Сведения о лице-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/complex-types/holder/2.0.1"
               schemaLocation="tHolder_v02.xsd"/>
    <!--КЛАССИФИКАТОРЫ И СПРАВОЧНИКИ:-->
    <!--Виды объектов реестра границ-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/directories/boundary-type/3.0.1"
               schemaLocation="dBoundaryType_v03.xsd"/>
    <!--Виды водных объектов-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/directories/water-object-type/1.0.1"
               schemaLocation="dWaterObjectType_v01.xsd"/>
    <!--Классификатор видов разрешенного использования земельных участков-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/directories/allowed-use/4.0.1"
               schemaLocation="dAllowedUse_v04.xsd"/>
    <!--Вид земельного участка-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/directories/parcels/2.0.1"
               schemaLocation="dParcels_v02.xsd"/>
    <!--Вид, тип зоны, территории-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/directories/zone/4.0.1"
               schemaLocation="dZone_v04.xsd"/>
    <!--Способ образования земельного участка-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/directories/method/2.0.1"
               schemaLocation="dMethod_v02.xsd"/>
    <!--Цели установления публичного сервитута-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/directories/purpose-public-easement/1.0.1"
               schemaLocation="dPurposePublicEasement_v01.xsd"/>
    <!--Категории земель-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/directories/categories/1.0.1"
               schemaLocation="dCategories_v01.xsd"/>
    <!--===ВЕРСИЯ 02===================-->
    <xs:element name="interact_entry_boundaries">
        <xs:annotation>
            <xs:documentation>Сведения, направляемые в орган регистрации прав в порядке информационного взаимодействия,
                для внесения в реестр границ Единого государственного реестра недвижимости (ЕГРН)
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="information_registry_boundaries" type="iBND2:InformationRegistryBoundaries">
                    <xs:annotation>
                        <xs:documentation>Сведения о границах, зонах, территориях, для внесения в реестр границ ЕГРН
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="guid" type="SimAll1:sGUID" use="required">
                <xs:annotation>
                    <xs:documentation>Глобальный уникальный идентификатор пакета (GUID)</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="version" type="SimAll1:s2" use="required" fixed="02">
                <xs:annotation>
                    <xs:documentation>Версия схемы</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="InformationRegistryBoundaries">
        <xs:annotation>
            <xs:documentation>Сведения о границах, зонах, территориях, для внесения в реестр границ ЕГРН
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="information_registry_boundary" type="iBND2:InformationRegistryBoundary"
                        maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Сведения о границе, зоне, территории, для внесения в реестр границ ЕГРН
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="InformationRegistryBoundary">
        <xs:annotation>
            <xs:documentation>Сведения о границе, зоне, территории, для внесения в реестр границ ЕГРН</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="type_boundary" type="dBnd3:dBoundaryType">
                <xs:annotation>
                    <xs:documentation>Вид объекта реестра границ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name_object" type="SimAll1:s1_1000" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование объекта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="all_border_or_part_border" type="SimAll1:sAllOrPartBorder" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Описание границы (1 - вся граница, 0 - часть (части) границы)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="information_boundary" type="iBND2:InformationBoundary">
                <xs:annotation>
                    <xs:documentation>Сведения о границе, зоне, территории</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="InformationBoundary">
        <xs:annotation>
            <xs:documentation>Сведения о границе, зоне, территории</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="state_boundary" type="iBND2:StateBoundary">
                <xs:annotation>
                    <xs:documentation>Сведения об установлении или изменении прохождения государственной границы
                        Российской Федерации
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="inhabited_locality_boundary" type="iBND2:InhabitedLocalityBoundary">
                <xs:annotation>
                    <xs:documentation>Сведения об установлении или изменении границы населенного пункта
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="surveying_project" type="iBND2:SurveyingProject">
                <xs:annotation>
                    <xs:documentation>Сведения об утверждении или изменении проекта межевания территории
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="coastline" type="iBND2:Coastline">
                <xs:annotation>
                    <xs:documentation>Сведения о береговой линии (границе водного объекта)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="zones_and_territories" type="iBND2:ZonesAndTerritories">
                <xs:annotation>
                    <xs:documentation>Сведения об установлении или изменении границы зоны или территории
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="public_easement" type="iBND2:PublicEasement">
                <xs:annotation>
                    <xs:documentation>Сведения об установлении или уточнении границы публичного сервитута
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="land_reserve" type="iBND2:LandReserve">
                <xs:annotation>
                    <xs:documentation>Сведения о границе территории резервирования земель или об изменении сведений о
                        такой границе
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <!--Сведения об установлении или изменении прохождения государственной границы Российской Федерации-->
    <xs:complexType name="StateBoundary">
        <xs:annotation>
            <xs:documentation>Сведения об установлении или изменении прохождения государственной границы Российской
                Федерации
            </xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="establishment_state_boundary" type="iBND2:EstablishmentStateBoundary">
                <xs:annotation>
                    <xs:documentation>Сведения об установлении прохождения государственной границы РФ</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="changing_state_boundary" type="iBND2:ChangingStateBoundary">
                <xs:annotation>
                    <xs:documentation>Сведения об изменении прохождения государственной границы РФ</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="EstablishmentStateBoundary">
        <xs:annotation>
            <xs:documentation>Установление прохождения участка государственной границы РФ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name_neighbouring_country" type="SimAll1:s1_500">
                <xs:annotation>
                    <xs:documentation>Полное наименование сопредельного иностранного государства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="international_treat_docs" type="RLaw1:DocumentRequisitesLaw">
                <xs:annotation>
                    <xs:documentation>Сведения о федеральном законе, которым ратифицирован международный договор
                        Российской Федерации, и о его официальном опубликовании
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="contours_location" type="EnSpa2:BoundContoursLocation" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Описание местоположения границы (графическое и, при необходимости,
                            текстовое)
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="location_text" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Текстовое описание местоположения границы</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ChangingStateBoundary">
        <xs:annotation>
            <xs:documentation>Изменение прохождения участка государственной границы РФ</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="reg_numb_border" type="SimAll1:s1_50">
                <xs:annotation>
                    <xs:documentation>Реестровый номер границы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="name_neighbouring_country" type="SimAll1:s1_500">
                <xs:annotation>
                    <xs:documentation>Полное наименование сопредельного иностранного государства</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="international_treat_docs" type="RLaw1:DocumentRequisitesLaw">
                <xs:annotation>
                    <xs:documentation>Сведения о федеральном законе, которым ратифицирован международный договор
                        Российской Федерации, и о его официальном опубликовании
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="contours_location" type="EnSpa2:BoundContoursLocation" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Описание местоположения границы (графическое и, при необходимости,
                            текстовое)
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="location_text" type="xs:string">
                    <xs:annotation>
                        <xs:documentation>Текстовое описание местоположения границы</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <!--Сведения о береговой линии (границе водного объекта)-->
    <xs:complexType name="Coastline">
        <xs:annotation>
            <xs:documentation>Сведения о береговой линии (границе водного объекта)</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="new_coastline" type="iBND2:NewCoastline">
                <xs:annotation>
                    <xs:documentation>Внесение сведений о береговой линии (границе водного объекта)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="changing_coastline" type="iBND2:ChangingCoastline">
                <xs:annotation>
                    <xs:documentation>Изменение сведений о береговой линии (границе водного объекта)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="NewCoastline">
        <xs:annotation>
            <xs:documentation>Внесение сведений о береговой линии (границе водного объекта)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="cadastral_district" type="SimAll1:sCadastralDistrictNull">
                <xs:annotation>
                    <xs:documentation>Номер кадастрового района</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="water" type="iBND2:Water">
                <xs:annotation>
                    <xs:documentation>Водный объект (тип, наименование)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contours_location" type="EnSpa2:BoundContoursLocation">
                <xs:annotation>
                    <xs:documentation>Описание местоположения береговой линии (границы водного объекта)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ChangingCoastline">
        <xs:annotation>
            <xs:documentation>Изменение сведений о береговой линии (границе водного объекта)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="reg_numb_border" type="SimAll1:s1_50">
                <xs:annotation>
                    <xs:documentation>Реестровый номер границы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cadastral_district" type="SimAll1:sCadastralDistrictNull">
                <xs:annotation>
                    <xs:documentation>Номер кадастрового района</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="water" type="iBND2:Water">
                <xs:annotation>
                    <xs:documentation>Водный объект (тип, наименование)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="contours_location" type="EnSpa2:BoundContoursLocationChange">
                <xs:annotation>
                    <xs:documentation>Описание местоположения береговой линии (границы водного объекта)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Water">
        <xs:annotation>
            <xs:documentation>Водный объект (тип, наименование)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="water_object_type" type="dWtr1:dWaterObjectType">
                <xs:annotation>
                    <xs:documentation>Тип водного объекта</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="water_object_name" type="SimAll1:s1_255" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Наименование водного объекта</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--Сведения о проекте межевания территории-->
    <xs:complexType name="SurveyingProject">
        <xs:annotation>
            <xs:documentation>Сведения об утверждении или изменении проекта межевания территории</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="establishment_surveying_project" type="iBND2:EstablishmentSurveyingProject">
                <xs:annotation>
                    <xs:documentation>Сведения об утверждении проекта межевания территории</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="changing_surveying_project" type="iBND2:ChangingSurveyingProject">
                <xs:annotation>
                    <xs:documentation>Сведения об изменении проекта межевания территории</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="EstablishmentSurveyingProject">
        <xs:annotation>
            <xs:documentation>Утверждение проекта межевания территории</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="quarter_cad_number" type="SimAll1:sCadastralQuarterNull">
                <xs:annotation>
                    <xs:documentation>Кадастровый номер квартала</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="forming_parcels">
                <xs:annotation>
                    <xs:documentation>Образуемые земельные участки</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="forming_parcel" type="iBND2:FormingParcel" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Образуемый земельный участок</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="modify_parcels" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Изменяемые земельные участки</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="modify_parcel" type="iBND2:ModifyParcel" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Изменяемый земельный участок</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="contours_location" type="EnSpa2:BoundContoursLocation" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Описание местоположения границы территории, в отношении которой утвержден проект
                        межевания
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ChangingSurveyingProject">
        <xs:annotation>
            <xs:documentation>Изменение проекта межевания территории</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="quarter_cad_number" type="SimAll1:sCadastralQuarterNull">
                <xs:annotation>
                    <xs:documentation>Кадастровый номер квартала</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="survey_project_num" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Учетный номер проекта межевания территории</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="forming_parcels" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Образуемые земельные участки</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="forming_parcel" type="iBND2:FormingParcel" maxOccurs="unbounded">
                            <xs:annotation>
                                <xs:documentation>Образуемый земельный участок</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="modify_parcels" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Изменяемые земельные участки</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="modify_parcel" type="iBND2:ModifyParcel" maxOccurs="unbounded">
                            <xs:annotation>
