                    <xs:documentation>Элемент контура</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SpelementUnit">
        <xs:annotation>
            <xs:documentation>Часть элемента</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="type_unit" type="dTopo1:dTypeTopology">
                <xs:annotation>
                    <xs:documentation>Вид топологии элемента</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ordinates" type="EnSpa1:Ordinates">
                <xs:annotation>
                    <xs:documentation>Список координат</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Ordinates">
        <xs:annotation>
            <xs:documentation>Список координат</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ordinate" type="EnSpa1:NewOrdinate" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Координата</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Ordinate">
        <xs:annotation>
            <xs:documentation>Координата</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="x" type="SimAll1:d38_2">
                <xs:annotation>
                    <xs:documentation>Координата X</xs:documentation>
