                </xs:annotation>
            </xs:element>
            <xs:element name="spatials_elements" type="EnSpa1:SpatialsElementsOldNew">
                <xs:annotation>
                    <xs:documentation>Элементы контура</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SpatialsElementsOldNew">
