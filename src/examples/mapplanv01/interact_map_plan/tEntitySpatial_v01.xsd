        </xs:annotation>
        <xs:sequence>
            <xs:element name="spatial_element" type="EnSpa1:SpelementUnitOldNew" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Элемент контура</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SpelementUnitOldNew">
        <xs:annotation>
            <xs:documentation>Часть элемента</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="type_unit" type="dTopo1:dTypeTopology">
                <xs:annotation>
                    <xs:documentation>Вид топологии элемента</xs:documentation>
