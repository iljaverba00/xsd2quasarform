                <xs:annotation>
                    <xs:documentation>Список координат</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OrdinatesOldNew">
        <xs:annotation>
            <xs:documentation>Список координат (существующие, новые (уточненные) координаты)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ordinate" type="EnSpa1:OrdinateOldNew" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Координата (существующая, новая, уточненная (измененная) координата)
