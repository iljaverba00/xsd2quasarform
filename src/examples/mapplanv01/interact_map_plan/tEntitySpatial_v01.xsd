            </xs:element>
            <xs:element name="delta_geopoint" type="SimAll1:d20_2" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Погрешность</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Borders">
        <xs:annotation>
            <xs:documentation>Текстовое описание прохождения границ, расположенных между двумя точками
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="border" type="EnSpa1:Border" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Участок границы, расположенный между двумя точками</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Border">
        <xs:annotation>
            <xs:documentation>Участок границы, расположенный между двумя точками</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="nmb_contour" type="SimAll1:p10">
                <xs:annotation>
                    <xs:documentation>Порядковый номер контура</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="spatial_element" type="SimAll1:p22">
                <xs:annotation>
                    <xs:documentation>Порядковый номер элемента контура</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="point1" type="SimAll1:p22">
                <xs:annotation>
                    <xs:documentation>Порядковый номер точки1 в элементе</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="point2" type="SimAll1:p22">
                <xs:annotation>
                    <xs:documentation>Порядковый номер точки2 в элементе</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="definition" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Описание прохождения участка границы</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--===Уточнение местоположения границ территорий, зон, объектов землеустройства-->
    <xs:complexType name="BoundContoursLocationOldNew">
        <xs:annotation>
            <xs:documentation>Текстовое и графическое описание местоположения (существующее/уточненное)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contours" type="EnSpa1:ContoursBoundariesOldNew">
                <xs:annotation>
                    <xs:documentation>Контуры (контур) (существующие, новые координаты характерных точек контура)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="borders" type="EnSpa1:BordersOldNew" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Текстовое описание местоположения (существующее/уточненное описание прохождения
