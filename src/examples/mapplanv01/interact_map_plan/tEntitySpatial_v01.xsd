                </xs:annotation>
            </xs:element>
            <xs:element name="num_geopoint" type="SimAll1:p22" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Номер точки (межевой точки)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="geopoint_zacrep" type="SimAll1:s120" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Способ закрепления точки</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="geopoint_opred" type="dGeO1:dGeopointOpred" minOccurs="0">
