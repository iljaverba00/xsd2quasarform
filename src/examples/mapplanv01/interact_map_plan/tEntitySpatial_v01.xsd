            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ContoursBoundariesOldNew">
        <xs:annotation>
            <xs:documentation>Контуры (контур) (существующие, новые координаты характерных точек контура)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contour" type="EnSpa1:ContourBoundariesOldNew" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Контур (существующие, новые координаты характерных точек контура)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ContourBoundariesOldNew">
        <xs:annotation>
            <xs:documentation>Описание контура границы</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="number_pp" type="SimAll1:p10" minOccurs="0">
