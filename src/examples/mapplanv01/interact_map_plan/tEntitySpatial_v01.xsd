        <xs:annotation>
            <xs:documentation>Текстовое и графическое описание местоположения</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contours" type="EnSpa1:ContoursBoundaries">
                <xs:annotation>
                    <xs:documentation>Контуры (контур) (координаты характерных точек контура)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="borders" type="EnSpa1:Borders" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Текстовое описание местоположения (описание прохождения границ, расположенных
                        между двумя точками)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="location_text" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Текстовое описание местоположения границы</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="BoundContours">
        <xs:annotation>
            <xs:documentation>Графическое описание местоположения границы (характерных точек границы)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contours" type="EnSpa1:ContoursBoundaries">
                <xs:annotation>
                    <xs:documentation>Контуры (контур) (координаты характерных точек контура)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ContoursBoundaries">
        <xs:annotation>
            <xs:documentation>Контуры (контур) (координаты характерных точек контура)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contour" type="EnSpa1:ContourBoundaries" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Контур (координаты характерных точек контура)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ContourBoundaries">
        <xs:annotation>
            <xs:documentation>Описание контура границы</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="number_pp" type="SimAll1:p10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Учетный (порядковый) номер контура границы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="definition" type="SimAll1:s50" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Обозначение контура границы</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="entity_spatial" type="EnSpa1:EntitySpatial">
                <xs:annotation>
                    <xs:documentation>Описание элементов контура (характерных точек контура)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="BoundContoursLocationBorders">
        <xs:annotation>
            <xs:documentation>Текстовое и графическое описание местоположения (для ОЗ)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contours" type="EnSpa1:ContoursBoundaries">
                <xs:annotation>
                    <xs:documentation>Контуры (контур) (координаты характерных точек контура)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="borders" type="EnSpa1:Borders">
                <xs:annotation>
                    <xs:documentation>Текстовое описание местоположения (описание прохождения границ, расположенных
                        между двумя точками)
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="location_text" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Текстовое описание местоположения границы</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--===Общие типы-->
    <xs:complexType name="EntitySpatial">
        <xs:annotation>
            <xs:documentation>Описание элементов контура (характерных точек контура)</xs:documentation>
