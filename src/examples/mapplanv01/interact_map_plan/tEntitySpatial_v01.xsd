           xmlns:dGeO1="urn://x-artefacts-rosreestr-ru/commons/directories/geopoint-opred/1.0.1"
           xmlns:dTopo1="urn://x-artefacts-rosreestr-ru/commons/directories/type-topology/1.0.1"
           targetNamespace="urn://x-artefacts-rosreestr-ru/commons/complex-types/entity-spatial/1.0.1"
           elementFormDefault="qualified">
    <!--ПРОСТЫЕ ТИПЫ ДАННЫХ:-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/simple-types-all/1.0"
               schemaLocation="P_CommonSimpleTypeAll_v01.xsd"/>
    <!--СОСТАВНЫЕ ТИПЫ ДАННЫХ:-->
    <!--Площадь-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/complex-types/area/1.0.1"
               schemaLocation="tArea_v01.xsd"/>
    <!--КЛАССИФИКАТОРЫ И СПРАВОЧНИКИ:-->
    <!--Методы определения координат характерных точек-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/directories/geopoint-opred/1.0.1"
               schemaLocation="dGeopointOpred_v01.xsd"/>
    <!--Виды топологии элемента-->
    <xs:import namespace="urn://x-artefacts-rosreestr-ru/commons/directories/type-topology/1.0.1"
               schemaLocation="dTypeTopology_v01.xsd"/>
    <!--===========Изменение:-->
    <!--Добавлен новый тип графического описания уточняемых границ-->
    <!--=====ВЕРСИЯ 1:-->
    <!--===Описание местоположения границ земельных участков-->
    <xs:complexType name="ZUContoursLocation">
        <xs:annotation>
            <xs:documentation>Описание местоположения границы земельного участка (характерных точек границы и участков
                границы от точки до точки)
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contours" type="EnSpa1:Contours">
                <xs:annotation>
                    <xs:documentation>Контуры (контур) границы (координаты характерных точек границы) земельного
                        участка
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="borders" type="EnSpa1:Borders" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Текстовое описание участков границы от точки до точки</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Contours">
        <xs:annotation>
            <xs:documentation>Контуры (контур) границы (координаты характерных точек границы)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="contour" type="EnSpa1:Contour" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Контур границы</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Contour">
        <xs:annotation>
            <xs:documentation>Описание контура границы</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="number_pp" type="SimAll1:p10" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Учетный (порядковый) номер контура</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="definition" type="SimAll1:s50" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Обозначение контура</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="area" type="Are1:LandAreaType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Площадь контура</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="entity_spatial" type="EnSpa1:EntitySpatial">
                <xs:annotation>
                    <xs:documentation>Описание элементов контура (характерных точек границы контура)</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--===Описание местоположения границ территорий, зон, объектов землеустройства-->
